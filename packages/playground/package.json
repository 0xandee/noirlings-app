{
  "name": "@andeebtceth/noirlings-app",
  "version": "0.8.0",
  "description": "A playground for the Noirlings exercises",
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc && vite build",
    "build:vercel": "tsc && vite build --mode vercel",
    "vercel-build": "yarn build:vercel",
    "build:watch": "tsc && vite build -w",
    "dev": "vite",
    "preview": "vite preview",
    "publish": "yarn build && yarn npm publish --access public",
    "server": "node --experimental-specifier-resolution=node src/server/server.js",
    "server:dev": "nodemon --experimental-specifier-resolution=node src/server/server.js",
    "start": "bash server.sh"
  },
  "dependencies": {
    "@monaco-editor/react": "4.6.0",
    "@noir-lang/backend_barretenberg": "0.25.0",
    "@noir-lang/noir_js": "0.25.0",
    "@noir-lang/noir_wasm": "0.25.0",
    "@noir-lang/types": "0.25.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fflate": "^0.8.1",
    "js-base64": "^3.7.5",
    "monaco-editor": "^0.44.0",
    "monaco-editor-textmate": "^4.0.0",
    "monaco-textmate": "^3.0.1",
    "onigasm": "^2.2.5",
    "react-toastify": "^9.1.3",
    "serve": "^14.2.4",
    "vercel": "^41.7.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "^20.10.6",
    "@types/react": "18.2.38",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "nodemon": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.2.2",
    "vite": "~4.4.9",
    "vite-plugin-dts": "^3.6.3"
  },
  "packageManager": "yarn@4.0.2"
}
